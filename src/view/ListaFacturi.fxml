<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="listaFacturiPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ListareFacturiController">
   <children>
      <HBox AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <Button fx:id="btnAdaugaFactura" defaultButton="true" mnemonicParsing="false" onAction="#facturaNoua" prefHeight="53.0" prefWidth="188.0" text="Adauga factura">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox layoutY="53.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <TextField prefHeight="44.0" prefWidth="839.0" promptText="Cauta factura">
               <font>
                  <Font size="20.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <TableView fx:id="tabelFacturi" layoutY="97.0" prefHeight="435.0" prefWidth="830.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <columns>
          <TableColumn fx:id="colNrCrt" maxWidth="1500.0" prefWidth="63.3333101272583" text="Nr. crt." />
          <TableColumn fx:id="colCodClient" prefWidth="176.6667251586914" text="Cod client" />
            <TableColumn fx:id="colCodBare" prefWidth="234.0" text="Cod bare" />
            <TableColumn fx:id="colSerieNr" prefWidth="393.0" text="Serie /Nr" />
            <TableColumn fx:id="colDataEmitere" prefWidth="393.0" text="Data emitere" />
            <TableColumn fx:id="colDataScadenta" prefWidth="393.0" text="Data scadenta" />
            <TableColumn fx:id="colSuma" prefWidth="393.0" text="Suma" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="BASELINE_RIGHT" layoutY="533.0" prefHeight="53.0" prefWidth="838.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <Button fx:id="btnInchide" alignment="BASELINE_RIGHT" contentDisplay="CENTER" graphicTextGap="6.0" mnemonicParsing="false" onAction="#inchide" style="-fx-background-color: #03f8fc;" text="Inchide">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
